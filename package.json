{
  "name": "whatsapp-session-pro",
  "version": "4.0.0",
  "description": "Professional WhatsApp Session Generator with Advanced Fallbacks",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "staging": "pm2 start ecosystem.config.js --env staging",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "reload": "pm2 reload ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs whatsapp-session-pro",
    "status": "pm2 status",
    "monitor": "pm2 monit",
    "setup": "npm install && node scripts/setup.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "build": "npm run lint && npm run test",
    "clean": "rm -rf node_modules logs sessions backups",
    "clean:all": "npm run clean && rm -rf package-lock.json",
    "backup": "node scripts/backup.js",
    "restore": "node scripts/restore.js",
    "health": "node scripts/health-check.js",
    "update": "npm update && npm audit fix",
    "security": "npm audit",
    "docker:build": "docker build -t whatsapp-session-pro .",
    "docker:run": "docker run -p 3000:3000 whatsapp-session-pro",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docs": "jsdoc -c jsdoc.json",
    "benchmark": "node benchmarks/load-test.js"
  },
  "keywords": [
    "whatsapp",
    "baileys",
    "session",
    "generator",
    "whatsapp-web",
    "whatsapp-bot",
    "pairing",
    "authentication",
    "abdullah-md"
  ],
  "author": "Abdullah-Md",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.5.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.0.3",
    "fs-extra": "^11.1.1",
    "uuid": "^9.0.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.0.0",
    "node-cache": "^5.1.2",
    "twilio": "^4.13.0",
    "nodemailer": "^6.9.3",
    "axios": "^1.4.0",
    "socket.io": "^4.6.1",
    "redis": "^4.6.7",
    "mongoose": "^7.3.0",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "validator": "^13.9.0",
    "rate-limiter-flexible": "^2.4.2",
    "winston": "^3.9.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "archiver": "^6.0.0",
    "unzipper": "^0.10.14",
    "crypto-js": "^4.1.1",
    "qrcode": "^1.5.3",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "chalk": "^4.1.2",
    "figlet": "^1.6.0",
    "cli-table3": "^0.6.3",
    "inquirer": "^8.2.6",
    "ora": "^5.4.1",
    "boxen": "^5.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "jsdoc": "^4.0.2",
    "@types/node": "^20.3.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "terser-webpack-plugin": "^5.3.9",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "dockerfilelint": "^1.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "commitlint": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdullah-md/whatsapp-session-pro.git"
  },
  "bugs": {
    "url": "https://github.com/abdullah-md/whatsapp-session-pro/issues"
  },
  "homepage": "https://abdullah-md.com/whatsapp-session-pro",
  "private": true,
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/abdullah-md"
  }
}
